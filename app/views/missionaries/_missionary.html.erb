<tr>
	<td><%= image_tag missionary.avatar(:medium) %></td>
  <td><%= missionary.missbio %></td>
  <td><%= link_to 'Show', missionary %></td>
  <% if current_user == missionary.user %>
  <td><%= link_to 'Edit', edit_missionary_path(missionary) %></td>
  <td><%= link_to 'Destroy', missionary, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  <% end %>
</tr>